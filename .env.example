# Environment variables template
# Copy this file to .env and fill in your values

# Required variables
VITE_MAPBOX_TOKEN=your_mapbox_token_here
VITE_API_URL=your_api_url_here
VITE_MONGODB_URI=your_mongodb_uri_here

# Note: For production, set these variables in Vercel's environment variables section
# Do not commit actual values to version control

# API URL (development only)
REACT_APP_API_URL=http://localhost:3001/api

# MongoDB connection string
REACT_APP_MONGODB_URI=mongodb+srv://username:password@cluster.mongodb.net/database

# Mapbox token for maps
REACT_APP_MAPBOX_TOKEN=your_mapbox_token_here

# Note: VERCEL_URL is automatically set by Vercel in production
# Only set this in .env.development for local testing
# VERCEL_URL=your-app.vercel.app 